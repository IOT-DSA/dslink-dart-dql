{
  "name": "dslink-dart-dql",
  "version": "0.6.0",
  "description": "DQL DSLink",
  "main": "bin/run.dart",
  "engines": {
    "dart": ">=1.12.0"
  },
  "getDependencies": [
    "pub get"
  ],
  "configs": {
    "broker": {
      "type": "url"
    },
    "name": {
      "type": "string",
      "default": "DQL"
    },
    "token": {
      "type": "string"
    },
    "log": {
      "type": "enum[info,off,warning,severe,fine,finer,finest,all]]",
      "default": "info"
    }
  }
}
