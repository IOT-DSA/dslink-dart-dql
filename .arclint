{
  "exclude": [
    "(packages/)",
    "(lib/addons/script.dart)"
  ],
  "linters": {
    "chmod": {
      "type": "chmod"
    },
    "spelling": {
      "type": "spelling"
    },
    "dart": {
      "type": "dart",
      "dart.ignore-files": [
        "lib/addons/script.dart"
      ],
      "dart.ignore-messages": [
        "Unnecessary brace in string interpolation"
      ],
      "dart.enable-linter": true
    },
    "merge-conflict": {
      "type": "merge-conflict"
    }
  }
}
